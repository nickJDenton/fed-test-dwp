{% extends "govuk/template.njk" %}

{% block head %}
  <!--[if !IE 8]><!-->
    <link href="stylesheets/style.css" rel="stylesheet">
  <!--<![endif]-->

{% endblock %}

{% block pageTitle %}
Address lookup - GOV.UK
{% endblock %}

{% block content %}
  <h1 class="govuk-heading-xl">Address lookup</h1>
  <div class="govuk-form-group">
    <label class="govuk-label" for="subject">
      Choose an address
    </label>
    <select class="govuk-select" id="subject" name="subject">
      <option value="choose" selected>Choose address</option>
      {% for item in listOfAddresses %}
   <option value="{{ item.GAZETTEER_ENTRY.NAMES_URI}} >{{ item.GAZETTEER_ENTRY.NAMES_URI}}</li>
{% endfor %}
      {# Display the addresses from the API call here #}
    </select>
  </div>
  <ul>
<ul>

</ul>
</ul>
{% endblock %}

{% block bodyEnd %}
  <script src="javascripts/javascript.js"></script>
{% endblock %}